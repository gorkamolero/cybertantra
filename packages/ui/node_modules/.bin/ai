#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/dist/bin/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/dist/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/dist/bin/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/dist/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules/ai/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/ai@5.0.0-beta.25_zod@4.0.10/node_modules:/Users/gorkolas/Documents/www/cybertantra/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ai/dist/bin/ai.min.js" "$@"
else
  exec node  "$basedir/../ai/dist/bin/ai.min.js" "$@"
fi
